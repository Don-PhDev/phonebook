<h1>List of contacts</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Name </th>
      <th>Phone Number </th>
      <th>Email </th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <th><%= contact.name %> </th>
        <th><%= contact.number %> </th>
        <th><%= contact.email %> </th>
      </tr>
    <%end%>
  </tbody>
</table>
<%= link_to "Add Contact", new_contact_path, class: "btn btn-secondary" %>
